select strftime('%m',usage_by_month.timestamp) as month,users.mac as mac, name, sum(data_in) as data_in,sum(data_out) as data_out,(select account from connections where user_id=users.id) as account from usage_by_month left join users on usage_by_month.user_id=users.id {1} group by month,usage_by_month.user_id;