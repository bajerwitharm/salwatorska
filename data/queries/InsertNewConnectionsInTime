INSERT INTO connections_in_time(user_id, start_timestamp, end_timestamp) select user_id,timestamp,(select timestamp from usage t2 where t2.id>=t1.id and not exists (select 1 from usage where user_id=t2.user_id and id>t2.id and timestamp<=DATETIME(t2.timestamp,'+60 minutes'))) from usage t1 where timestamp>ifnull((select max(end_timestamp) from connections_in_time),'1974-01-01') and not exists (select 1 from usage where user_id=t1.user_id and timestamp<t1.timestamp and timestamp>=DATETIME(t1.timestamp,'-60 minutes')) order by timestamp,user_id;