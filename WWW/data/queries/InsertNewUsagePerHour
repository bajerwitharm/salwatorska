INSERT INTO usage_by_hour(timestamp,user_id, data_in, data_out) 
SELECT 
  to_timestamp(to_char(timestamp,'YYYY-MM DD HH24:00'),'YYYY-MM DD HH24:MI') as timestamp,
  user_id,
  sum(data_in) as data_in,
  sum(data_out) as data_out
from usage_in_time 
where timestamp>COALESCE(
   (select max(timestamp) from usage_by_hour),
   '1974-01-01'
)
group by user_id, to_char(timestamp,'YYYY-MM DD HH24:00'); 